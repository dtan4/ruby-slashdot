(function () {
    if (typeof window.sdmedia !== 'object') window.sdmedia = {};
    if (typeof window.sdmedia.site !== 'object') window.sdmedia.site = {};

    var site = window.sdmedia.site;
    site.rootdir = "//slashdot.org";
